{
  "name": "whatsai-bot",
  "version": "1.0.0",
  "description": "A modular WhatsApp bot with Gemini AI for automated responses. Features customizable personalities, conversation memory, and session persistence.",
  "main": "src/index.js",
  "bin": {
    "whatsai-setup": "./setup.js"
  },
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "setup": "node setup.js",
    "test": "echo \"No tests specified\" && exit 0",
    "validate": "node -c src/index.js && node -c setup.js",
    "check-json": "node -e \"JSON.parse(require('fs').readFileSync('package.json', 'utf8')); JSON.parse(require('fs').readFileSync('config/default.json', 'utf8')); console.log('JSON files are valid!')\"",
    "lint": "eslint src/ setup.js --ext .js",
    "lint:fix": "eslint src/ setup.js --ext .js --fix",
    "format": "prettier --write src/ setup.js",
    "format:check": "prettier --check src/ setup.js"
  },
  "keywords": [
    "whatsapp",
    "ai",
    "bot",
    "nodejs",
    "gemini",
    "automation",
    "chatbot",
    "personality",
    "conversation",
    "memory",
    "session",
    "persistence"
  ],
  "author": "WhatsApp AI Bot Contributors",
  "license": "MIT",
  "homepage": "https://github.com/yourusername/whatsai-bot#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/whatsai-bot.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/whatsai-bot/issues"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@google/generative-ai": "^0.2.1",
    "dotenv": "^16.3.1",
    "fs-extra": "^11.1.1",
    "qrcode-terminal": "^0.12.0",
    "whatsapp-web.js": "^1.33.2",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "nodemon": "^3.0.2",
    "prettier": "^3.6.2"
  }
}
